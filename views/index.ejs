<div class="registre">
  <br>
   <h5><strong> Registre </strong></h5>
  <form action="/register" method="POST">
    <h6>Nom d'usuari</h6>
      <input type="text" name="username" placeholder="Username" required="required" class="formControl">
    <h6>Nom</h6>
      <input type="text" name="nomUsuari" placeholder="Nom" required="required" class="formControl">
    <h6>Cognoms</h6>
      <input type="text" name="cognoms" placeholder="Cognoms" required="required" class="formControl">
    <h6>Correu electrònic</h6>
      <input type="email" name="mail" placeholder="Mail" required="required" class="formControl">
    <h6>Contrasenya</h6>
      <input type="password" name="password" placeholder="Password" required="required" class="formControl">
      </br>
      <br>
    <button class="btn btn-primary">Registrar</button>
  </form> 
